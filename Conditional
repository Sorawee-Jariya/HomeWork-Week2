    }
    return "Not Equal";
  }
    testStrict(10);

3.Practice comparing different values

// Finish and Paste your solution here
// Setup
function compareEquality(a, b) {
    if (a === b) { // Change this line
      return "Equal";
    }
    return "Not Equal";
  }
    compareEquality(10, "10");

